# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                 controllers.Index.index

# Entry
GET     /entry            controllers.Entry.submit
POST    /entry            controllers.Entry.accept

# Filer
GET     /filer            controllers.Filer.upload(id)

# Result
GET     /table            controllers.Table.result

# Comments
GET     /board            controllers.Board.board

# FAQ
GET     /quest            controllers.Quest.quest

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file     controllers.Assets.versioned(path="/public", file: Asset)
